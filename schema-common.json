{
  "$schema": "http://json-schema.org/schema#",
  "id": "https://fiware.github.io/dataModels/schema-common.json",
  "title": "Common definitions for FIWARE Harmonized Data Models",
  "definitions": {
    "GSMA-Commons": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "description": "NGSI Entity id"
        },
        "dateCreated": {
          "type": "string",
          "format": "date-time"
        },
        "dateModified": {
          "type": "string",
          "format": "date-time"
        },
        "source": {
          "type": "string"
        },
        "name": {
          "type": "string"
         },
        "dataProvider": {
          "type": "string"
        }
      }
    },
    "Location-Commons": {
      "type": "object",
      "properties": {
        "location": {
          "type": "object"
        },
        "address": {
          "type": "object"
        }
      }
    }
  }
}